#
# (c) holds no legal value ;)
#
#
#
snippet =>
	${1:key} => ${2:value}
#
snippet =>,
	${1:key} => ${2:value},${3}
#
snippet :q
	'${1:key}': '${2:value}'${3}
#
snippet :qq
	"${1:key}": "${2:value}"${3}
#
snippet :q,
	'${1:key}': '${2:value}', ${3}
#
snippet :qq,
	"${1:key}": "${2:value}"${3}
#		
snippet $]
	$${1:arr}[${2}]${3}
#		
snippet $]q
	$${1:arr}['${2}']${3}
#		
snippet $]qq
	$${1:arr}["${2}"]${3}
#
# comment block
snippet --| 
	|
	| ------------------------------------------------------
	| ${1} 
	| ------------------------------------------------------
	|
	| ${2} 
	|
# comment inline for php, java, javascript	
snippet **
	/* ${1} */
snippet ***
	/* 
	 * ${1} 
	 */
# comment for php, java, javascript
snippet **** 
	/*
	*
	* ------------------------------------------------------
	* ${1} 
	* ------------------------------------------------------
	*
	* ${2} 
	*
	*/
# comment for php, ruby, python 
snippet ### 
	#
	#
	# ------------------------------------------------------
	# ${1} 
	# ------------------------------------------------------
	#
	# ${2} 
	#
snippet c)
	`&enc[:2] == "utf" ? "Â©" : "(c)"` Copyright `strftime("%Y")` ${1:`g:snips_author`}. All Rights Reserved.${2}
snippet date_
	`strftime("%Y-%m-%d")`
snippet time_
	`strftime("%H:%M:%S")`
snippet datetime_
	`strftime("%Y-%m-%d %H:%M:%S")`
snippet filename_
	`Filename()`
# block 
snippet nn
		
		${1}

# ()
snippet ))
	(
		${1}
	)
# {}		
snippet }}
	{
		${1}
	}
# []		
snippet ]]
	[
		${1}
	]
